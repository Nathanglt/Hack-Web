					<footer
						class="text-center text-lg-start bg-light text-muted">
						<!-- Copyright -->
						<div class="text-center p-4" style="background-color: rgba(0, 0, 0, 0.05)" , position="low">
							Adrien ABDERRAHMANN - Nathan GUILLET - Jules MARTIN
						</div>
						<!-- Copyright -->
					</footer>
					<!-- Footer -->
				{% endblock %}
